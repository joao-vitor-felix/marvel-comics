{
  "presets": [
    [
      "@babel/preset-env",
      { "targets": { "esmodules": true, "node": "current" } }
    ],
    ["@babel/preset-typescript"],
    ["@babel/preset-react", { "runtime": "automatic" }]
  ],
  "plugins": [
    "babel-plugin-transform-import-meta",
    "babel-plugin-transform-vite-meta-env"
  ]
}
